<?xml version="1.0"?>

<launch>

  <!-- Load RQT with a pre-setup GUI for Baxter controls from a perspective file  -->
  <node name="maurice_rqt" pkg="rqt_gui" type="rqt_gui" respawn="false"
	output="screen" args="--perspective-file $(find maurice_control)/launch/maurice_rqt.perspective"/>

</launch>
